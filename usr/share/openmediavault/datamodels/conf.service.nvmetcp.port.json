{
  "type": "config",
  "id": "conf.service.nvmetcp.port",
  "title": "nvmetcp port",
  "queryinfo": {
    "xpath": "/config/services/nvmetcp/ports/port",
    "iterable": true,
    "idproperty": "uuid"
  },
  "properties": {
    "uuid": {
      "type": "string",
      "format": "uuidv4"
    },
    "addr": {
      "type": "string"
    },
    "nport": {
      "type": "integer",
      "minimum": 1,
      "maximum": 65535,
      "default": 4420
    },
    "adrfam": {
      "type": "string",
      "enum": [
        "ipv4",
        "ipv6"
      ],
      "default": "ipv4"
    }
  },
  "required": [
    "addr",
    "nport",
    "adrfam"
  ]
}
